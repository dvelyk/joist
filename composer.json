{
  "name": "dvelyk/joist",
  "description": "Joist is a WordPress starter theme built with Timber",
  "type": "wordpress-theme",
  "minimum-stability": "stable",
  "authors": [
      {
          "name": "Kyle Robertson",
          "email": "kyle@typefirst.ca"
      }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "extra" : {
    "installer-paths" : {
      "../../plugins/{$name}/" : ["type:wordpress-plugin"]
    }
  },
  "require": {
    "wpackagist-plugin/safe-svg": "1.9.*",
    "timber/timber": "1.18.1",
    "mindkomm/timmy": "^0.14.5",
    "htmlburger/carbon-fields": "^3.1.20"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "0.7.*",
    "wp-coding-standards/wpcs": "2.3.*",
    "hellonico/timber-dump-extension": "1.0.*",
    "squizlabs/php_codesniffer": "3.5.*",
    "php-stubs/wordpress-stubs": "^5.4"
  }
}
